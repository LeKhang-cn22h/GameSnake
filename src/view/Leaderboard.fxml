<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RankingController">
   <children>
      <VBox layoutX="7.0" layoutY="7.0" prefHeight="301.0" prefWidth="700.0">
         <children>
            <Button fx:id="btnBack" mnemonicParsing="false" onAction="#proceedToGame" text="Quay lại" />
            <TableView fx:id="tableView" prefHeight="271.0" prefWidth="700.0">
              <columns>
                <TableColumn fx:id="rankColumn" prefWidth="75.0" text="Hạng" />
                <TableColumn fx:id="usernameColumn" prefWidth="224.0" text="Người chơi" />
                <TableColumn fx:id="scoreColumn" prefWidth="220.0" text="Điểm" />
                <TableColumn fx:id="dateColumn" prefWidth="165.60009765625" text="Ngày" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
